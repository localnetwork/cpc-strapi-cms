        "shared.image-with-stats",
        "shared.media-text",
        "shared.card-texts-block",
        "shared.timeline",
        "shared.text",
        "shared.interstitial-content",
        "shared.accordion",
        "shared.landscape-media-text",
        "shared.key-people",
        "shared.our-people",
        "shared.media",
        "shared.cards-with-accordion",
        "shared.banner-media-text",
        "shared.portrait-media-text",
        "shared.media-card"
