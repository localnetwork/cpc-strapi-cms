{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "default": "news/"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "Gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Metatags": {
      "type": "component",
      "repeatable": false,
      "component": "shared.meta"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "GAD",
        "Publication",
        "SAO",
        "Registrar"
      ],
      "required": true
    }
  }
}
