    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "social_links": {
      "type": "component",
      "repeatable": true,
      "component": "shared.site-info-social"
    },
    "Address": {
      "type": "text"
    },
    "Phone": {
      "type": "text"
    },
    "Email": {
      "type": "email"
    }
  }
}
